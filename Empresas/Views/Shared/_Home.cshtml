                    // Seleccionar los caracteres a partir del parámetro de la función
                    switch (valid) {
                        case 'num':
                            return (numeros.indexOf(String.fromCharCode(tecla)) > -1);
                            break;
                        case 'car':
                            return (caracteres.indexOf(String.fromCharCode(tecla)) > -1);
